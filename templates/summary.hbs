{{#if details.indicator.latestPanVerdicts.WF_SAMPLE}}
  {{#if (eq details.indicator.latestPanVerdicts.WF_SAMPLE "MALWARE")}}
    <span {{on "click" (optional this.scrollToIntegration)}} class="integration-summary-tag integration-background-color integration-border-color">
       <span class="acronym integration-text-bold-color">{{block.acronym}}</span>
       <span class="integration-text-color">MALWARE</span>
    </span>
  {{else}}
    <span {{on "click" (optional this.scrollToIntegration)}} class="integration-summary-tag integration-background-color integration-border-color">
       <span class="acronym integration-text-bold-color">{{block.acronym}}</span>
       <span class="integration-text-color">BENIGN</span>
    </span>
  {{/if}}
{{/if}}
